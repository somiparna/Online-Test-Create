<body class="">
  <div class="wrapper ">
    <div class="sidebar" data-color="orange">
      <div class="logo">
        <a href="" class="simple-text logo-mini"></a>
        <div class="simple-text logo-normal">
          <%if(type==0){%>
            Admin
          <%}%>
          <%if(type==1){%>
            Client
            <%}%>
          <%if(type==2){%>
            Test Setter
          <%}%>
          <%if(type==3){%>
            Test Checker
          <%}%>
        </div>
      </div>
      <div class="sidebar-wrapper">
         <ul class="nav">
          <li>
            <a href="/dashboard">
              <i class="now-ui-icons design_app"></i>
              <p>Dashboard</p>
            </a>
          </li>
          <%if(type==0){%>
          <li class="active">
            <a href="/manage_client">
              <i class="now-ui-icons education_atom"></i>
              <p>
               Clients
              </p>
            </a>
          </li>
          <%}%>
          <%if(type==0 || type==1){%>
           <li>
            <a href="/manage_exam">
              <i class="now-ui-icons location_map-big"></i>
              <p>Exams</p>
            </a>
          </li>
           <li>
            <a href="/manage_users">
              <i class="now-ui-icons ui-1_bell-53"></i>
              <p>Test Setters</p>
            </a>
          </li>
           <li>
            <a href="/manage_checker">
              <i class="now-ui-icons ui-1_bell-53"></i>
              <p>Test Checkers</p>
            </a>
          </li>
          <%}%>
          <%if(type==0 || type==1 || type==2){%>
           <li>
            <a href="/manage_qa">
              <i class="now-ui-icons users_single-02"></i>
              <p>Question Papers</p>
            </a>
          </li>
          <%}%>
          <%if(type==0 || type==1){%>
           <li>
            <a href="/manage_students">
              <i class="now-ui-icons design_bullet-list-67"></i>
              <p>Students</p>
            </a>
          </li>
          <%}%>
          <%if(type==0 || type==1 || type==3){%>
           <li>
            <a href="/manage_result">
              <i class="now-ui-icons text_caps-small"></i>
              <p>Check Responses</p>
            </a>
          </li>
          <%}%>
          <%if(type==0 || type==1){%>
           <li>
            <a href="/final_result">
              <i class="now-ui-icons users_single-02"></i>
              <p>Final Results</p>
            </a>
          </li>
          <%}%>
          <li>
            <a href="/logout">
              <i class="now-ui-icons text_caps-small"></i>
              <p>Logout</p>
            </a>
          </li>
        </ul>
      </div>
    </div>
    <div class="main-panel">
      <div class="panel-header panel-header-md">
        <h1 class="text-center text-white">Manage Clients</h1>
      </div>
      <div class="content">
        <div class="row">
          <div class="col-md-12">
            <div class="card ">
              <div class="card-body ">
                <form method="POST" action="/manage_client">
                  <div class="row">
                    <div class="col-md-6">
                  <label>Enter the name of the client</label>
                  <div class="form-group">
                    <input class="form-control" placeholder="Enter the name" name="name" type="text" required>
                  </div>
                </div>
                <div class="col-md-6">
                  <label>Set Username</label>
                  <div class="form-group">
                    <input class="form-control" placeholder="Username" name="username" type="text" required>
                  </div>
                </div>
              </div>
              <div class="row">
                    <div class="col-md-6">
                  <label>Enter the profession</label>
                  <div class="form-group">
                    <input class="form-control" placeholder="Enter the profession" list="profession" name="profession">
                  </div>
                </div>
                <div class="col-md-6">
                 <label>Password</label>
                  <div class="form-group">
                    <input class="form-control" placeholder="Password" name="password" type="password" id="pass" required>
                  </div>
                </div>
              </div>
              <div class="row">
                    <div class="col-md-6">
                  <label>Enter the mobile number</label>
                  <div class="form-group">
                    <input class="form-control" placeholder="Enter the Mobile Number" name="mob" type="number" required>
                  </div>
                </div>
                <div class="col-md-6">
                  <label>Enter the Aadhaar number</label>
                  <div class="form-group">
                    <input class="form-control" placeholder="Enter the Aadhar Number" name="aadhar" type="text" required>
                  </div>
                </div>
              </div>
              <div class="card-footer text-center">
                <button type="submit" class="btn btn-fill btn-primary">Add Client</button>
              </div>
                </form>
              </div>
            </div>
          </div>
          <div class="col-md-12">
            <button type="submit" id="myFunction" class="btn btn-fill btn-primary">View Clients</button>
          </div>
        </div>

        <div id="cli_list" class="col-md-12 col-lg-12" style="margin-top: 30px;">
          <div class="card">
            <div class="card-header">
              <h4 class="card-title">Clients List</h4>
            </div>
            <div class="card-body">
              <div class="toolbar">
                <!--        Here you can write extra buttons/actions for the toolbar              -->
              </div>
              <table class="table table-striped table-bordered" cellspacing="0" width="100%">
                <thead>
                  <tr>
                  <th>Client Id</th>
                  <th>Client Name</th>
                  <th>Mobile Number</th>
                  <th>Aadhar Number</th>
                  <th>User Name</th>
                  <th>User Password</th>
                  </tr>
                </thead>
                <tfoot>
                  <tr>
                    <th><strong>Client Id</strong></th>
                    <th><strong>Client Name</strong></th>
                    <th><strong>Mobile Number</strong></th>
                    <th><strong>Aadhar Number</strong></th>
                    <th><strong>User Name</strong></th>
                    <th><strong>User Password</strong></th>
                  </tr>
                </tfoot>
                <tbody>
                  <%if(users.length){
                    for(var i = 0;i < users.length;i++) { %>
                      <tr>
                        <td><%=users[i].client_id%></td>
                        <td><%=users[i].client_name%></td>
                        <td><%=users[i].mob_no%></td>
                        <td><%=users[i].aadhar_no%></td>
                        <td><%=users[i].username%></td>
                        <td><%=users[i].password%></td>
                      </tr>
                    <% }
                  } %>
                </tbody>
              </table>
            </div>
          </div>
        </div>

      </div>
      <footer class="footer">
        <div class="container-fluid">
          <div class="copyright" id="copyright">
            &copy;
            <script>
              document.getElementById('copyright').appendChild(document.createTextNode(new Date().getFullYear()))
            </script>, Designed and Coded by
            <a href="javascript:void(0)" target="_blank">Group 10</a>.
          </div>
        </div>
      </footer>
    </div>
  </div>
