                    <tr>
                      <th>Exam ID</th>
                      <th>Exam Name</th>
                      <%if(type==0){%>
                        <th>Client Name</th>
                      <%}%>
                      <th>Total Marks</th>
                      <th>Duration</th>
                      <th class="disabled-sorting">Check</th>
                    </tr>
                  </thead>
                  <tfoot>
                    <tr>
                      <th>Exam ID</th>
                      <th>Exam Name</th>
                      <%if(type==0){%>
