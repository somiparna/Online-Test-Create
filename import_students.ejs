    <div class="main-panel">
      <div class="panel-header panel-header-md">
        <h1 class="text-center text-white">Import Students</h1>
      </div>
      <div class="content">
        <div class="row">
          <div class="col-md-12">
            <div class="card ">
              <div class="card-body ">
                <form method="post" action="/import_students" enctype="multipart/form-data">
                <div class="col-md-12">
                  <label>Import the csv file</label>
                    <div class="custom-file mb-3">
                      <input type="file" class="custom-file-input form-control" name="csv" id="fileupload" required>
                      <label class="custom-file-label form-control" for="customFile" style="margin-top: 3px ;">Choose file</label>
                    </div>
                </div>
                <div class="card-footer text-center">
                  <button type="submit" class="btn btn-fill btn-primary">Add Student</button>
                </div>
                </form>
              </div>
            </div>
          </div>
        </div>

        <% if(method == "POST") {%>
        <div id="stu_list" class="col-md-12 col-lg-12" style="margin-top: 50px;">
          <div class="card">
            <div class="card-header">
              <h4 class="card-title">Students List</h4>
            </div>
            <div class="card-body">
              <div class="toolbar">
                <!--        Here you can write extra buttons/actions for the toolbar              -->
              </div>

                <table class="table table-striped table-bordered" cellspacing="0" width="100%"" id="exam">
                  <thead>
                    <tr>
                    <th>Reg No</th>
                    <th>Name</th>
                    <th>Mobile No</th>
                    <th>Exam</th>
                    <th>Email</th>
                    </tr>
                  </thead>
                  <tfoot>
                  <tr>
                    <th><strong>Reg No</strong></th>
                    <th><strong>Name</strong></th>
                    <th><strong>Mobile No</strong></th>
                    <th><strong>Exam</strong></th>
                    <th><strong>Email</strong></th>
                  </tr>
                  </tfoot>
                  <tbody>
                    <% if(students!=null){
                      if(students.length>0){
                        for(var i = 0;i < students.length;i++) { %>
                          <tr>
                            <td><%=students[i].reg_no%></td>
                            <td><%=students[i].stu_name%></td>
                            <td><%=students[i].mob_no%></td>
                            <td><%=students[i].exam_name%></td>
                            <td><%=students[i].email%></td>
                          </tr>
                        <%}
                      }
                    } %>
                  </tbody>
                </table>
              
          </div>
        </div>
      </div>
      <% } %>

